<div id="printerhistory">
    <h3>Database Settings</h3>
    <div class="form-group">
        <label class="control-label col-sm-3" for="inputUser">User:</label>
        <div class="col-sm-9">
            <input type="text" class="form-control" id="inputUser" data-bind="value: settingsViewModel.settings.printerHistory.database.user">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3" for="inputPassword">Password:</label>
        <div class="col-sm-9">
            <input type="password" class="form-control" id="inputPassword" value: "settingsViewModel.settings.printerHistory.database.password">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3" for="inputHost">Host:</label>
        <div class="col-sm-9">
            <input type="text" class="form-control" id="inputHost" data-bind="value: settingsViewModel.settings.printerHistory.database.host">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3" for="inputPort">Port:</label>
        <div class="col-sm-9">
            <input type="number" class="form-control" id="inputPort" data-bind="value: settingsViewModel.settings.printerHistory.database.port">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-3" for="inputDatabase">Database:</label>
        <div class="col-sm-9">
            <input type="text" class="form-control" id="inputDatabase" data-bind="value: settingsViewModel.settings.printerHistory.database.database">
        </div>
    </div>
</div>
